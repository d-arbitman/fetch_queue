{
  "extends": ".es5-eslintrc",
  "rules": {
    "constructor-super": 2,
    "generator-star-spacing": [
      2,
      {
        "before": true,
        "after": false
      }
    ],
    "no-inner-declarations": 0,
    "no-lone-blocks": 0,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 2
  }
}
